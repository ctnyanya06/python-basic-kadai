        "#　台形の上辺と下辺、高さを定義\n",
        "up=10\n",
        "do=20\n",
        "hi=5\n",
        "\n",
        "#　台形の面積を計算\n",
        "area = (up + do) * hi / 2\n",
        "\n",
        "#　面積の表示\n",
        "print(str(area) + \"cm²\")\n"
